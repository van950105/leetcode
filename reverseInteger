class Solution:
    # @return an integer
    def reverse(self,x):
        s=str(x)
        if s[0]=="-":
            s="-"+s[:0:-1]
        else:
            s=s[::-1]
        return int(s)
        
#[::-1]might not be an elegent way to do it
#transfer into string might cost too much but simpler :/
